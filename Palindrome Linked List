# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        heads = []
        while head:
            heads.append(head.val)
            head.next
        i = 0
        j = len(heads)-1
        
        while i<=j:
            if heads[i] != heads[j]:
                return False
            i+=1
            j-=1
        return True
    
        
